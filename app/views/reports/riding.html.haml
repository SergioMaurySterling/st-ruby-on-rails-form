  }

  .container .checkbox label {
    min-height: initial;
    min-width: 12px;
    padding: 0;
    width: 12px;
    height: 12px;
    cursor: pointer;
    position: absolute;
    top: 0;
    left: 0;
    background: #f8f9fa;
    border: solid 1px #dae1ea;
  }

  .checkbox label:after {
    content: '';
    width: 11px;
    height: 5px;
    position: absolute;
    top: 4px;
    left: 1px;
    border: 3px solid #333;
    border-top: none;
    border-right: none;
    background: transparent;
    opacity: 0;
    zoom: 75%;
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
  }

  .checkbox label:hover::after {
    opacity: 0.5;
  }

  .checkbox input[type=checkbox] {
    visibility: hidden;
  }

  .checkbox input[type=checkbox]:checked + label:after {
    opacity: 1;
  }


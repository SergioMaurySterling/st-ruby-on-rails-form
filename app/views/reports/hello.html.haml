  }

  .container .panel-primary {
    margin-bottom: 15px;
  }


  .container .panel-primary .panel-heading {
    color: #fff;
    background-color: #337ab7;
    border-color: #337ab7;
  }

  .container table.lights td:last-child {
    vertical-align: middle;
    border-left: 1px solid #dae1ea;
  }

  .container .checkbox label {
    min-height: initial;
    min-width: 12px;
    padding: 0;
    width: 12px;
    height: 12px;
    cursor: pointer;
    position: absolute;
    top: 0;
    left: 0;
    background: #f8f9fa;
    border: solid 1px #dae1ea;
  }

  .checkbox label:after {
    content: '';
    width: 11px;
    height: 5px;
    position: absolute;
    top: 4px;
    left: 1px;
    border: 3px solid #333;
    border-top: none;
    border-right: none;
    background: transparent;
    opacity: 0;
    zoom: 75%;
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
  }

  .checkbox label:hover::after {
    opacity: 0.5;
  }

  .checkbox input[type=checkbox] {
    visibility: hidden;
  }

  .checkbox input[type=checkbox]:checked + label:after {
    opacity: 1;
  }

  .container table {
    margin-bottom: 0;
  }

  .container table > tbody > tr > td > span {
    padding: 3px;
    margin: 0;
    display: flex;
  }

  .container table > tbody > tr {
    border-top: 1px solid #dae1ea;
    border-bottom: 1px solid #dae1ea;
  }

  .container table > tbody > tr > td {
    min-height: 16px;
    height: 16px;
    border: none !important;
  }

  .container table > tbody > tr > td .col span {
    line-height: 12px;
    margin-left: 2px;
  }

  .container table > tbody > tr > td, .container table > tbody > tr > td * {
    margin: 0;
    padding: 0;
    vertical-align: middle;
    max-height: 16px;
    height: 16px;
  }

  .container table > tbody > tr.double > td, .container table > tbody > tr.double > td * {
    margin: 0;
    padding: 0;
    vertical-align: middle;
    height: initial;
    min-height: 12px;
    max-height:25px;
  }

  .container table > tbody > tr.tripple > td, .container table > tbody > tr.tripple > td * {
    margin: 0;
    padding: 0;
    vertical-align: middle;
    height: initial;
    min-height: 12px;
    max-height: 36px;
  }

  .container table > tbody > tr.double > td, .container table > tbody > tr.tripple > td {
    display: flex;
    flex-direction:column;
  }

  .container table > tbody > tr.double .col-xs-4, .container table > tbody > tr.tripple .col-xs-4 {
    padding: 0 2px;
  }

  .container table > tbody > tr.double .row span, .container table > tbody > tr.tripple .row span {

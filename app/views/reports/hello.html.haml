:css
  @font-face {
    font-family: 'FontAwesome';
    src: url('./font/fontawesome-webfont.ttf?v=4.7.0') format('truetype');
    font-weight: normal;
    font-style: normal;
  }
  .flex-row {
    display: flex;
  }

  span.comment-block {
    white-space: pre-line;
    padding: 5px;
  }
  .container .row.comment-row {
    justify-content: left;
  }

  .wrapper * {
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    font-size: 6px;
    line-height: 6px;
    font-weight: 500;
    text-align: left;car.png
    color: #4e5c6f;
  }

  .wrapper {
    width: 658px;
    position: relative;
    margin: 0 auto;
    padding: 22px 30px;
    border: none;
  }

  .container {
    height: 100%;
    width: 100%;
    margin: 10px 0;
    font-size: 7px;
    font-weight: 500;
    max-width: 568px;
  }

  header.header {
    width: 568px;
    height: 40px;
    border: solid 1px #dae1ea;
    padding: 2px;
    text-align: center;
  }

  .header img {
    height: 34px;
  }

  header.sub-header {
    display: flex;
    justify-content: space-between;
    margin-top: 6px;
  }

  header.sub-header h3 {
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    font-size: 12px;
    font-weight: bold;
    text-align: left;
    color: #4e5c6f;
    display: flex;
    margin: 0;
    padding: 0;
    height: 15px;
    line-height: 16px;
  }

  header.sub-header h5 {
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    font-size: 8px;
    font-weight: bold;
    text-align: left;
    color: #4e5c6f;
    display: flex;
    margin: 0;
    padding: 0;
    height: 15px;
    line-height: 15px;
  }

  header.sub-header h5 + header.sub-header h7,
  header.sub-header h7 {
    margin-left: 6px;
    text-transform: uppercase;
  }

  header.sub-header h7 {
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    font-size: 6px;
    font-weight: 500;
    text-align: left;
    color: #4e5c6f;
    height: 15px;
    line-height: 15px;
  }

  fieldset label {
    font-weight: inherit;
    margin: 0;
  }

  fieldset input {
    border: none;
    border-bottom: 1px solid #dae1ea;
    margin: 0;
    padding: 0;
  }

  .container {
    height: 100%;
    width: 100%;
    margin-bottom: 10px;
    font-size: 7px;
    font-weight: 500;
    color: #4e5c6f;
    padding: 0;

  }

  .container .row {
    margin: 0;
    display: flex;
    flex-direction: row;
    justify-content: center;
  }

  .container input {
    border: none;
    line-height: 6px;
    height: 10px;
    border-bottom: 1px solid #dae1ea;
    width: 100%;
  }

  .sub-header {
    justify-content: center;
    display: flex;
    flex: 1 0 auto;
  }

  .container .col {
    display: flex;
    flex: 1 0 50%;
    flex-direction: column;
  }

  .container .col:first-of-type {
    padding-right: 5px;
  }
  .container .col:not(:first-of-type) {
    padding-left: 5px;
  }

  .container .form-group-sm.col {
    flex-direction:row;
  }

  .container .form-group-sm {
    display: flex;
    align-content: space-between;
    width: 100%;
  }

  .container .row > .form-group-sm:first-of-type,
  .container .row > .form-group-sm > label:first-of-type:not(:last-of-type) {
    padding-left: 0;
    padding-right: 5px;
  }

  .container .row > .form-group-sm:last-of-type,
  .container .row > .form-group-sm > label:first-of-type:not(:last-of-type) {
    padding-right: 0;
    padding-left: 5px;
  }

  .container .row > .form-group-sm:last-of-type:first-of-type {
    padding: 0 5px;
  }

  .container .form-group-sm.col {
    display: flex;
    flex: 1 1 auto;
    max-width: 50%;
  }

  .container .form-group-sm,
  .container .form-group-sm * {
    padding: 0 5px;
  }

  .container .form-group-sm label {
    flex: 1 0 auto;
    margin: 5px 0;
    line-height: 5px;
    text-transform: uppercase;
  }

  .legend {
    width: 568px;
    height: 22px;
    border: solid 1px #dae1ea;
    margin: 0 0 2px 0;
  }

  .legend .col-xs-4 {
    display: flex;
    /*margin-right: 2px;*/
    padding: 5px;
    justify-content: center;
  }

  .container .panel-primary .panel-body {
    padding: 0;
  }

  .container .panel.comments, .container .panel.comments .panel-body {
    border: none;
    box-shadow: none;
  }

  .container .panel.comments .row > .form-group-sm:last-of-type:first-of-type {
    padding: 0;
    margin-left: -5px;
  }

  .container .panel-primary .col-xs-4, .container .panel-primary .col {
    padding: 2px;
  }

  .container .inspect-disclaimer {
    line-height: 12px;
  }

  .legend .col-xs-4:last-of-type, .container .panel-primary .col:last-of-type {
    margin-right: 0;
  }

  .legend span {
    padding: 0 2px;
    flex: 1 1 auto;
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    font-size: 7px;
    font-weight: 500;
    text-align: left;
    color: #4e5c6f;
    line-height: 10px;
  }

  .col-center i.status {
    display: flex;
    align-self: center;
  }

  .legend .going-green {
    display: flex;
    flex: 1 1 auto;
    justify-content: center;
  }

  .legend .going-green span {
    flex: initial;
  }

  .container .green {
    background-color: #5cc7a3;
  }

  .container .yellow {
    background-color: #fdd927;
  }

  .container .red {
    background-color: #fd275d;
  }

  .container .sm-red-circle {
    width: 8px;
    height: 8px;
    border-radius: 100px;
    background-color: #fd275d;
    border: solid 1px #dadde6;
    display:flex;
  }


  .container .sm-yellow-circle {
    width: 8px;
    height: 8px;
    border-radius: 100px;
    border: solid 1px #dadde6;
    background-color: #fdd927;
  }

  .container .sm-green-circle {
    width: 8px;
    height: 8px;
    border-radius: 100px;
    background-color: #5cc7a3;
    border: solid 1px #dadde6;
  }

  i.status.complete:before{
    margin-left:-3px !important;
    color: #ffffff;
    font-size: 6px;
    font-family: "FontAwesome";
    font: normal normal normal 6px/1 FontAwesome;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;

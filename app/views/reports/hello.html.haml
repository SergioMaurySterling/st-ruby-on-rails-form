  .container table > tbody > tr > td .col-space:last-of-type span {
    line-height: 16px;
  }

  .container table > tbody > tr > td .col-space {
    padding-left: 3px;
  }

  tr.double, tr.tripple {
    min-height:35px;
  }

  tr.double .row, tr.tripple .row {

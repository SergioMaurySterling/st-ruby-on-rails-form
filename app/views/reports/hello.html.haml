  }

  .container .panel {
    border-radius: 0;
    margin: 0;
    padding: 0;
    border: 1px solid #dae1ea;
    width: 100%;
  }

  .container .panel-heading {
    padding: 0 5px;
    border-radius: 0;
    /*border: 1px solid #327AB9;*/
    border-bottom: none;
    text-align: center;
    font-size: 6px;
    text-transform: uppercase;
    height: 15px;
